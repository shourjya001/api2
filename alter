-- Drop the comment on the column "CURRENCY_FORMAT"
COMMENT ON COLUMN dbo."TMODCURLOGDBE"."CURRENCY_FORMAT" IS NULL;

-- Drop the new column "CURRENCY_FORMAT"
ALTER TABLE dbo."TMODCURLOGDBE"
